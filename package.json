{
  "name": "shine-squad",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "prod": "vite preview",
    "fresh-install": "rm -rf node_modules .vite && rm -f package-lock.json && npm install",
    "build-server": "rm -rf dist && tsc",
    "server": "nodemon ./dist/server/server.js",
    "app-start": "npm run build && npm run build-server && concurrently \"npm run prod\" \"npm run server\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@formspree/react": "^3.0.0",
    "@mui/icons-material": "^7.1.0",
    "@mui/material": "^7.3.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "fraction.js": "^5.2.2",
    "ipaddr.js": "^2.2.0",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.11",
    "nodemon": "^3.1.10",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.60.0",
    "react-router-dom": "^7.5.0",
    "vite-express": "^0.21.1",
    "web-vitals": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/cors": "^2.8.19",
    "@types/dotenv": "^8.2.3",
    "@types/multer": "^2.0.0",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^19.0.12",
    "@types/react-bootstrap": "^1.0.1",
    "@types/react-dom": "^19.0.4",
    "@types/react-spinner": "^0.2.4",
    "@vitejs/plugin-react": "^4.7.0",
